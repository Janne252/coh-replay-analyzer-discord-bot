{
    "cache": false,
    "extension": [".ts"],
    "check-coverage": true,
    "include": ["src/**/*.ts"],
    "exclude": ["src/**/*.spec.ts"],
    "statements": 90,
    "branches": 90,
    "functions": 95,
    "lines": 90,
    "temp-dir": ".nyc.temp",
    "report-dir": ".nyc.coverage",
    "all": true
}